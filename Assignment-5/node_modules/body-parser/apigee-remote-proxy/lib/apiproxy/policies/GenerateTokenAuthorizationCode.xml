<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OAuthV2 name="GenerateTokenAuthorizationCode">
    <DisplayName>GenerateTokenAuthorizationCode</DisplayName>
    <Operation>GenerateAccessToken</Operation>
    <ExpiresIn ref="request.header.x-DNA-Token-Lifetime">3600000</ExpiresIn>
    <SupportedGrantTypes>
      <GrantType>authorization_code</GrantType>
    </SupportedGrantTypes>
    <GenerateResponse enabled="true"/>
  <Attributes>
    <Attribute name="attributes" ref="request.header.x-DNA-Token-Attributes"></Attribute>
  </Attributes>
</OAuthV2>