<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<APIProxy revision="1" name="apigee-remote-proxy">
    <ConfigurationVersion minorVersion="0" majorVersion="4"/>
    <Description>Proxy for Apigee Remote operations</Description>
    <DisplayName>Apigee Remote Proxy</DisplayName>
    <Policies>
        <Policy>ExtractInvalidateParams</Policy>
        <Policy>GenerateAuthorizationCode</Policy>
        <Policy>GenerateTokenAuthorizationCode</Policy>
        <Policy>GenerateTokenClientCredentials</Policy>
        <Policy>GenerateTokenImplicit</Policy>
        <Policy>GenerateTokenPasswordCredentials</Policy>
        <Policy>GenerateVerifyResponse</Policy>
        <Policy>InvalidateRefreshToken</Policy>
        <Policy>InvalidateToken</Policy>
        <Policy>RefreshToken</Policy>
        <Policy>Return404</Policy>
        <Policy>SetEmptyResponse</Policy>
        <Policy>VerifyAPIKey</Policy>
        <Policy>VerifyToken</Policy>
        <Policy>DistributedQuota</Policy>
        <Policy>ExtractQuotaParams</Policy>
        <Policy>SetQuotaResponse</Policy>
        <Policy>AssignOAuthVariables</Policy>
    </Policies>
    <ProxyEndpoints>
        <ProxyEndpoint>default</ProxyEndpoint>
    </ProxyEndpoints>
    <Resources/>
    <TargetServers/>
    <TargetEndpoints/>
    <validate>false</validate>
</APIProxy>
